# AI/ML System Environment Configuration
# Add these variables to your .env file

# ============================================
# FEATURE FLAGS
# ============================================
# Enable AI/ML system (set to 1 to enable)
VITE_FEATURE_AI_ML=1

# ============================================
# SUPABASE CONFIGURATION
# ============================================
# Your Supabase project URL
SUPABASE_URL=your_supabase_project_url_here

# Supabase service role key (NEVER expose to browser)
# This is used for server-side operations only
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Supabase anon key (safe for browser)
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================
# AI/ML MODEL CONFIGURATION
# ============================================
# Model training configuration
AI_ML_TRAIN_SPLIT=0.8
AI_ML_VALIDATION_SPLIT=0.2
AI_ML_EPOCHS=100
AI_ML_BATCH_SIZE=32
AI_ML_LEARNING_RATE=0.001
AI_ML_MIN_SAMPLES=100
AI_ML_RETRAIN_THRESHOLD=50

# Prediction thresholds
AI_ML_BUY_THRESHOLD=0.6
AI_ML_SELL_THRESHOLD=0.4

# Feature engineering periods
AI_ML_RSI_PERIOD=14
AI_ML_EMA_FAST_PERIOD=12
AI_ML_EMA_SLOW_PERIOD=26
AI_ML_ATR_PERIOD=14
AI_ML_VOLUME_SMA_PERIOD=20

# ============================================
# STORAGE CONFIGURATION
# ============================================
# Supabase storage bucket for models
AI_ML_STORAGE_BUCKET=ai-ml-models

# ============================================
# DEVELOPMENT/DEBUGGING
# ============================================
# Enable debug logging for AI/ML system
AI_ML_DEBUG=false

# Enable detailed TensorFlow logging
TF_CPP_MIN_LOG_LEVEL=1

# ============================================
# EXAMPLE VALUES (DO NOT USE IN PRODUCTION)
# ============================================
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# VITE_FEATURE_AI_ML=1
